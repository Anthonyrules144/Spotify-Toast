{
    "name": "spotify_toast",
    "productName": "Spotify Toast",
    "description": "Spotify-Toast but made in Node.JS",
    "version": "0.0.2",
    "repository": {
        "url": "http://www.github.com/Ashleyz4/Spotify-Toast"
    },
    "license": "MIT",
    "author": {
        "name": "Ashleyz4",
        "url": "http://www.github.com/Ashleyz4"
    },
    "keywords": [
        "Spotify",
        "Toast",
        "Electron"
    ],
    "main": "main.ts",
    "scripts": {
        "start": "electron .",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
        "package-win": "electron-packager . spotify-toast --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
        "package-linux": "electron-packager . spotify-toast --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
    },
    "dependencies": {
        "electron": "^2.0.0",
        "electron-packager": "^12.0.2",
        "events": "^2.0.0",
        "request": "^2.85.0",
        "querystring": "^0.2.0",
        "util": "^0.10.3",
        "path": "^0.12.7",
        "child_process": "^1.0.2",
        "fs": "^0.0.1-security",
        "jquery": "^3.3.1"
    },
    "devDependencies": {
        "electron-packager": "^12.0.2"
    }
}
