{
	"name": "spotify-toast",
	"productName": "Spotify-Toast",
	"version": "0.1.6",
	"description": "Spotify-Toast but made in Node.JS",
	"main": "./src/main.ts",
	"scripts": {
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make --platform zip",
		"make-mac": "electron-forge make --platform darwin",
		"make-linux-deb": "electron-forge make --platform linux-deb",
		"make-linux-rpm": "electron-forge make --platform linux-rpm",
		"lint": "echo \"No linting configured\"",
		"help": "node help.js"
	},
	"repository": {
		"url": "http://www.github.com/Ashleyz4/Spotify-Toast"
	},
	"markdown": "github",
	"readme": "readme.md",
	"bugs": {
		"url": "http://www.github.com/Ashleyz4/Spotify-Toast/issues/"
	},
	"keywords": [],
	"author": {
		"name": "Ashleyz4",
		"url": "http://www.github.com/Ashleyz4/"
	},
	"license": "MIT",
	"config": {
		"forge": {
			"make_targets": {
				"zip": [
					"zip"
				],
				"darwin": [
					"dmg"
				],
				"linux-deb": [
					"deb"
				],
				"linux-rpm": [
					"rpm"
				]
			},
			"electronPackagerConfig": {
				"packageManager": "npm"
			},
			"electronWinstallerConfig": {
				"name": "app"
			},
			"electronInstallerDebian": {},
			"electronInstallerRedhat": {},
			"github_repository": {
				"owner": "Ashleyz4",
				"name": "Spotify-Toast"
			}
		}
	},
	"dependencies": {
		"child_process": "^1.0.2",
		"electron-compile": "^6.4.2",
		"electron-squirrel-startup": "^1.0.0",
		"events": "^2.0.0",
		"fs": "^0.0.1-security",
		"jquery": "^3.3.1",
		"mocha": "^5.1.1",
		"node-vibrant": "^3.0.0",
		"path": "^0.12.7",
		"querystring": "^0.2.0",
		"request": "^2.85.0",
		"uglify-js": "3.3.24",
		"util": "^0.10.3"
	},
	"devDependencies": {
		"babel-plugin-transform-async-to-generator": "^6.24.1",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"electron-forge": "^5.2.1",
		"electron-prebuilt-compile": "2.0.0",
		"eslint": "^3.19.0",
		"eslint-config-airbnb": "^15.1.0",
		"eslint-plugin-import": "^2.11.0",
		"eslint-plugin-jsx-a11y": "^5.1.1",
		"eslint-plugin-react": "^7.7.0"
	}
}
